gin: 0 auto;\r\n    .box {\r\n      width: 80px;\r\n      height: 80px;\r\n      margin: 4px;\r\n      overflow: hidden;\r\n      background-color: #555;\r\n      border: 3px solid #fff;\r\n      border-radius: 10px;\r\n      box-sizing: border-box;\r\n      transform: skewX(-14deg);\r\n      transition: transform 0.1s ,\r\n                  background-color 0.6s;\r\n      &:hover {\r\n        background-color: #ff9c00;\r\n        transform: skewX(-14deg) scale(1.3);\r\n        z-index: 1;\r\n      }\r\n      .image {\r\n        display: flex;\r\n        width: 100%;\r\n        height: 100%;\r\n        background-size: cover;\r\n        background-repeat: no-repeat;\r\n        transform: skewX(14deg);\r\n      }\r\n      @for $i from 1 through 32 {\r\n        &:nth-child(#{$i}) .image {\r\n          background-image: url(\"#{$url}/hero#{$i}.png\");\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n/* #footer */\r\n#footer {\r\n  padding: 20px; \r\n  background: transparent;\r\n  .copyright { \r\n    text-align: center; \r\n    color: #2c4348;\r\n    div:first-child {\r\n      font-size: 1.5em;\r\n      font-family: Gaegu; \r\n      font-weight: bold;\r\n    }\r\n    div:last-child {\r\n      font-size: 1.3em; \r\n      margin-top: 5px;\r\n      font-family: Gaegu; \r\n      font-weight: bold;\r\n    }\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"83217a9f73b4dbfc61a4826b17ac4638","cacheData":{"env":{}}}